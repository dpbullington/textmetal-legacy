{
	"version": "9.0.0-*",
	"description": "TextMetal Middleware Unit Tests Console App",
	"authors":
	[
		"Â©2002-2016 Daniel Bullington (dpbullington@gmail.com)"
	],

	"packOptions":
	{
		"licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
		"iconUrl": "https://github.com/dpbullington/textmetal/blob/master/doc/textmetal-logo.png",
		"projectUrl": "http://www.textmetal.com",
		"summary": "A zero friction, open source meta-programming framework for .NET.",
		"requireLicenseAcceptance": false,
		"tags":
		[
			"TextMetal"
		],
		"repository":
		{
			"type": "git",
			"url": "https://github.com/textmetal/main"
		}
	},

	"buildOptions":
	{
		"debugType": "portable",
		"keyFile": "../TextMetal.snk",
		"warningsAsErrors": false,
		"emitEntryPoint": true,
		"compile":
		{
			"exclude":
			[
				"Datazoid"
			]
		},
		"copyToOutput":
		[
			"appconfig.json"
		]
	},

	"frameworks":
	{
		"netcoreapp1.1":
		{
			"dependencies":
			{
				"Microsoft.NETCore.App":
				{
					"version": "1.1.0",
					"type": "platform"
				}
			}
		}
	},

	"dependencies":
	{
		"TextMetal.Imports": "9.0.0-*",
		"TextMetal.Middleware.Solder.Context": "9.0.0-*",
		"TextMetal.Middleware.Solder.Executive": "9.0.0-*",
		"TextMetal.Middleware.Solder.Extensions": "9.0.0-*",
		"TextMetal.Middleware.Solder.Injection": "9.0.0-*",
		"TextMetal.Middleware.Solder.Interception": "9.0.0-*",
		"TextMetal.Middleware.Solder.Primitives": "9.0.0-*",
		"TextMetal.Middleware.Solder.Serialization": "9.0.0-*",
		"TextMetal.Middleware.Solder.Utilities": "9.0.0-*",
		"TextMetal.Middleware.Datazoid": "9.0.0-*"
	},

	"commands":
	{
		"test": "TextMetal.Middleware.UnitTests"
	}
}
