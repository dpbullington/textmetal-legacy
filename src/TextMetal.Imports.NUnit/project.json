{
	"version": "9.0.0-*",
	"description": "TextMetal Imports NUnit Library",
	"authors":
	[
		"Â©2002-2017 Daniel P. Bullington (dpbullington@gmail.com)"
	],

	"packOptions":
	{
		"licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
		"iconUrl": "https://github.com/dpbullington/textmetal/blob/master/doc/textmetal-logo.png",
		"projectUrl": "http://www.textmetal.com",
		"summary": "A zero friction, open source meta-programming framework for .NET.",
		"requireLicenseAcceptance": false,
		"tags":
		[
			"TextMetal"
		],
		"repository":
		{
			"type": "git",
			"url": "https://github.com/textmetal/main"
		}
	},

	"buildOptions":
	{
		"debugType": "portable",
		"keyFile": "../TextMetal.snk",
		"warningsAsErrors": false,
		"compile":
		{
			"exclude":
			[
				"Framework\\Compatibility\\System\\Lazy.cs",
				"Framework\\Compatibility\\System.Threading\\SpinWait.cs",
				"Lite\\OutputWriters",
				"Lite\\DebugWriter.cs",
				"Lite\\OutputManager.cs",
				"Lite\\TcpWriter.cs"
			]
		}
	},

	"frameworks":
	{
		"netstandard1.6":
		{
			"buildOptions":
			{
				"define":
				[
					"NETSTANDARD1_6",
					"PORTABLE",
					"NUNIT_FRAMEWORK",
					"NUNITLITE"
				]
			},

			"dependencies":
			{
			}
		}
	},

	"dependencies":
	{
		"NETStandard.Library": "1.6.1",
		"System.Console": "4.3.0",
		"System.Runtime.Serialization.Primitives": "4.3.0",
		"System.Xml.XmlDocument": "4.3.0"
	}
}
