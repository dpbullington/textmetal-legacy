{
	"version": "9.0.0-*",
	"description": "TextMetal Imports NUnit Library",
	"authors": [ "Â©2002-2016 Daniel Bullington (dpbullington@gmail.com)" ],

	"packOptions": {
		"licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
		"iconUrl": "https://github.com/dpbullington/textmetal/blob/master/doc/textmetal-logo.png",
		"projectUrl": "http://www.textmetal.com",
		"summary": "A zero friction, open source meta-programming framework for .NET.",
		"requireLicenseAcceptance": false,
		"tags": [ "TextMetal" ],
		"repository": {
			"type": "git",
			"url": "https://github.com/textmetal/main"
		}
	},

	"buildOptions": {
		"keyFile": "../TextMetal.snk",
		"warningsAsErrors": false,
		"compile":
		{
			"exclude":
			[
				"Common\\Compatibility\\BindingFlags.cs",
				"Lite\\OutputWriters",
				"Lite\\DebugWriter.cs",
				"Lite\\OutputManager.cs",
				"Lite\\TcpWriter.cs",
				"Lite\\TeamCityEventListener.cs"
			]
		}
	},

	"frameworks": {
		"netstandard1.5": {
			"buildOptions": {
				"define": [ "PORTABLE", "NUNIT_FRAMEWORK", "NUNITLITE" ]
			},

			"dependencies": {
				"NETStandard.Library": "1.5.0-rc2-*",
				"System.Console": "4.0.0-rc2-24027",
				"System.Runtime.Serialization.Primitives": "4.1.1-rc2-24027",
				"System.Xml.XmlDocument": "4.0.1-rc2-24027"
			}
		}
	},

	"dependencies": {
	}
}
